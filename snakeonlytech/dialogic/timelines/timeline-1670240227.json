		{
			"character": "",
			"event_id": "dialogic_001",
			"portrait": "",
			"text": "\"Don't worry about my heart.\""
		},
		{
			"event_id": "dialogic_023",
			"hide_dialogbox": true,
			"wait_seconds": 0.2
		},
		{
			"event_id": "dialogic_024",
